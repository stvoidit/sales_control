{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedLocals": false
  },
  "include": [
    "src/**/*.ts"
  ]
}
